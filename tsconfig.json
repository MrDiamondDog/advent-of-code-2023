{
    "compilerOptions": {
        "target": "es2016",
        "lib": ["es2021"],
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "moduleResolution": "node",
        "resolveJsonModule": true
    }
}
